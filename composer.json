{
    "name": "rmanara/brutalforce",
    "type": "library",
    "license": "MIT",
    "version": "2.0.1",
    "description": "help on secure website against brutal force attack",
    "keywords": [
        "brute-force",
        "ip track",
        "firewall",
        "blocking",
        "spam"
    ],
    "authors": [{
        "name": "Rodrigo Manara",
        "email": "me@rodrigomanara.co.uk",
        "homepage": "https://rodrigomanara.co.uk"
    }],
    "homepage": "https://github.com/rodrigomanara/brutalforce",
    "require": {
        "php": ">=5.6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "8.*"
    },
    "autoload": {
        "psr-4": {
            "BrutalForce\\": "src/BrutalForce"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrutalForce\\Test\\": "tests/"
        }
    },
    "archive": {
        "exclude": ["vendor", "test"]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}